{
  "name": "apiconnect-mgmt-lite-datapower",
  "version": "1.1.9",
  "description": "Using docker-compose start DataPower and some minimal API Connect Management server functions to aid in the test and development of APIs using the toolkit and DataPower specific policies",
  "main": "index.js",
  "scripts": {
    "up": "APICONNECT_MGMT_LITE_PORT_9443=4002:9443 TARGET_URL=http://$(HOSTNAME):4001 docker-compose up",
    "down": "docker-compose down",
    "start": "node index.js",
    "lint": "eslint .",
    "test": "nyc --reporter=cobertura --reporter=text mocha test/index.js",
    "apigateway-test": "nyc --reporter=cobertura --reporter=text mocha test/index.js --grep \"v5 Tests\"",
    "no-e2e": "mocha --grep \"/End-to-End|Docker/\" --invert",
    "pretest": "npm run lint",
    "postinstall": "node ./update-env.js",
    "prepublish": "node ./update-env.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.ibm.com:apimesh/apiconnect-mgmt-lite-datapower.git"
  },
  "keywords": [
    "datapower",
    "apiconnect",
    "apic",
    "ibm",
    "api",
    "management"
  ],
  "author": "IBM API Connect",
  "license": "SEE LICENSE IN LICENSE.txt",
  "dependencies": {
    "apiconnect-apigateway-framework": "~1.2.x",
    "apiconnect-config-to-datapower": "~2.0.0",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "chokidar": "^1.6.1",
    "dockerode": "^2.3.1",
    "escape-string-regexp": "^1.0.5",
    "express": "^4.14.0",
    "fs-extra": "^2.1.2",
    "glob": "^7.1.1",
    "js-yaml": "^3.9.0",
    "lodash": "^4.17.4",
    "needle": "^1.4.5",
    "pem": "^1.9.4",
    "request": "^2.79.0",
    "uuid": "^3.0.1",
    "which": "^1.3.0"
  },
  "devDependencies": {
    "auth-header": "^0.2.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.13.1",
    "eslint-config-loopback": "^7.0.1",
    "methods": "^1.1.2",
    "mocha": "^3.1.2",
    "npm": "^3.10.10",
    "nyc": "^11.0.1",
    "ps-node": "^0.1.3",
    "rewire": "^2.5.2",
    "semver": "^5.4.1",
    "supertest": "^2.0.1"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}

{
  "name": "strong-trace",
  "version": "1.3.4",
  "description": "StrongTrace Node.js Tracer",
  "main": "strong-trace.js",
  "scripts": {
    "test": "tap  --bail --coverage --coverage-report=cobertura --timeout=200 test/test-*.js",
    "posttest": "nyc report --reporter=lcov && nyc report"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "StrongLoop <engineering@strongloop.com>",
  "jshintConfig": {
    "asi": true,
    "globalstrict": true,
    "validthis": true,
    "eqnull": true,
    "node": true,
    "loopfunc": true,
    "newcap": false,
    "eqeqeq": false
  },
  "devDependencies": {
    "bcrypt": "^0.8.1",
    "isnumber": "^1.0.0",
    "nyc": "^3.0.0",
    "stats-lite": "^1.0.3",
    "tap": "^0.7.0",
    "tape": "^2.14.0"
  },
  "dependencies": {
    "esprima": "^1.2.2",
    "estraverse": "^1.5.0",
    "glob-to-regexp": "0.0.1",
    "stats-incremental": "^1.1.0",
    "statware": "~1.1.2",
    "strongloop-license": "^1.0.4",
    "transaction-tracer": "~1.1.0",
    "xtend": "~4.0.0"
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "homepage": "https://strongloop.com",
  "repository": {
    "type": "git",
    "url": "git://github.com/strongloop/strong-trace.git"
  },
  "keywords": [
    "strongloop",
    "monitoring",
    "tracing",
    "monitor",
    "trace",
    "waterfall",
    "profile",
    "performance",
    "metrics",
    "flamegraph",
    "transaction",
    "delay",
    "debug",
    "instrument",
    "require",
    "transform",
    "rewrite",
    "transpile",
    "scope",
    "ast",
    "esprima",
    "functions",
    "tracer",
    "aggregation",
    "transactions",
    "api"
  ],
  "bugs": {
    "url": "https://github.com/strongloop/strong-trace/issues"
  }

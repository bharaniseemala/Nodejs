{
  "name": "apiconnect-cli-util",
  "version": "7.1.5",
  "description": "A set of utilities used by the apic toolkit clis",
  "keywords": [
    "apiconnect",
    "apic",
    "toolkit apic",
    "ibm apiconnect",
    "ibm api connect"
  ],
  "main": "index.js",
  "scripts": {
    "pretest": "eslint --ignore-path .gitignore .",
    "cov": "tap --bail --timeout 20000 --cov --coverage-report=lcov test/test-*.js",
    "test": "tap --bail --timeout 20000 --reporter=spec test/test-*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.ibm.com/apimesh/apiconnect-cli-util.git"
  },
  "dependencies": {
    "apiconnect-apim-client": "^3.x",
    "apiconnect-collective-controller-api": "^2.x",
    "apiconnect-config": "^1.x",
    "bluebird": "^1.x",
    "chance": "0.7.7",
    "debug": "2.2.x",
    "lodash": "4.0.x",
    "request-promise": "^2.0.1",
    "strong-globalize": "^2.x"
  },
  "devDependencies": {
    "eslint": "^2.x",
    "eslint-config-apiconnect": "^2.x",
    "mocha": "2.2.5",
    "nock": "^7.x",
    "sinon": "^1.x",
    "sinon-as-promised": "^4.x",
    "tap": "^5.7.0"
  },
  "engines": {
    "node": ">=4"
  },
  "author": "IBM API Management",
  "license": "SEE LICENSE IN LICENSE.txt"
}

{
  "name": "apiconnect-cli-pm",
  "version": "2.4.6",
  "description": "Plugin for IBM API Connect Developer Toolkit",
  "main": "index.js",
  "scripts": {
    "pretest": "eslint .",
    "test": "tap --bail --timeout=10000 --reporter=spec ./test/test-*",
    "cov": "tap --bail --timeout 20000 --cov --coverage-report=lcov test/test-*.js",
    "start": "node index.js",
    "docs": "jsdoc2md --src index.js **/*.js > api-docs.md",
    "test-cli-format": "CLI_FORMAT_FILE=cli-format.md tap test/test-help.js"
  },
  "keywords": [
    "apic-toolkit-plugin",
    "apim",
    "apiconnect",
    "apic",
    "plugin",
    "ibm",
    "api",
    "management"
  ],
  "author": "IBM API Connect",
  "license": "SEE LICENSE IN LICENSE.txt",
  "dependencies": {
    "apiconnect-cli-logger": "^1.x",
    "apiconnect-cli-util": "^7.x",
    "apiconnect-collective-controller-api": "^2.x",
    "apiconnect-config": "^1.x",
    "apiconnect-mgmt-lite-datapower": "^1.x",
    "apiconnect-project": "^1.x",
    "async": "^1.x",
    "bluebird": "^3.x",
    "debug": "^2.x",
    "fs-extra": "~0.26.5",
    "inquirer": "~0.11.x",
    "install": "~0.3.0",
    "lodash": "^4.x",
    "microgateway": "^1.x",
    "mkdirp": "~0.5.1",
    "osenv": "~0.1.3",
    "request": "^2.69.0",
    "rimraf": "^2.5.2",
    "strong-deploy": "^3.x",
    "strong-globalize": "^2.x",
    "strong-mesh-models": "^8.x",
    "strong-pm": "^6.x",
    "text-table": "~0.2.0"
  },
  "devDependencies": {
    "apiconnect-cli-auth": "^3.x",
    "apiconnect-cli-test-support": "^4.x",
    "boom": "^3.1.2",
    "collab-help": "^1.0.0",
    "eslint": "^2.x",
    "eslint-config-apiconnect": "^2.x",
    "express": "^4.13.4",
    "md5": "^2.1.0",
    "nock": "^8.0.0",
    "shelljs": "^0.6.0",
    "sinon": "^1.x",
    "sinon-as-promised": "^4.x",
    "tap": "^5.x",
    "temp": "^0.8.3"
  },
  "engines": {
    "node": ">=4"

{
  "name": "apiconnect-project",
  "version": "1.0.19",
  "description": "Configuration module IBM API Connect Developer Toolkit",
  "main": "index.js",
  "scripts": {
    "pretest": "eslint .",
    "test": "tap --bail --timeout=1000 ./test/test-*",
    "cov": "tap --bail --timeout 20000 --cov --coverage-report=lcov test/test-*.js",
    "start": "node index.js",
    "test-cli-format": "CLI_FORMAT_FILE=cli-format.md tap test/test-help.js",
    "docs": "jsdoc2md --src **/*.js > api-docs.md"
  },
  "keywords": [
    "apim",
    "apiconnect",
    "apic",
    "plugin",
    "ibm",
    "api",
    "management"
  ],
  "author": "IBM API Connect",
  "license": "SEE LICENSE IN LICENSE.txt",
  "dependencies": {
    "bluebird": "^3.4.3",
    "debug": "^2.2.0",
    "glob": "^7.0.5",
    "js-yaml": "^3.6.1",
    "json-schema-deref": "0.3.7",
    "strong-globalize": "^2.6.75",
    "traverse": "^0.6.6"
  },
  "devDependencies": {
    "eslint": "^2.x",
    "eslint-config-apiconnect": "^2.x",
    "jsdoc-to-markdown": "^1.3.7",
    "lodash": "^4.15.0",
    "tap": "^6.3.2"
  },
  "engines": {
    "node": ">=4"
  }
}
